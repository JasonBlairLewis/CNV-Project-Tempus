dataFrame2$"Chromosome"<-2
startSeq<-seq(0,243000000,1000000)
endSeq<-seq(5000000,243000000,1000000)
for (p in 1:239){
  scipen<-999
  dataFrame2$`Start Position`[p]<-startSeq[p]
  dataFrame2$`End Position`[p]<-endSeq[p]
}


#qual subsets data from a specific cancer type and specific chromosome by position on chromosome to create a smaller data frame of 
#of dat from only genes with start positions between a 5,000,000 bp  window, and then finds the mean of the Gain/Loss column of that subset and Assigns a column in data frame 
#respective data. 

for (k in 1:28){
  for (i in 1:239){
    qual<-subset(get(paste0(cancerNames[k],2)),start_position<=endSeq[i]&start_position>=startSeq[i])
    print(qual)
    gain<-mean(as.numeric(qual$Gain))
    loss<-mean(as.numeric(qual$Loss))
    dataFrame2[i,k*4+4]<-as.numeric(gain)
    dataFrame2[i,k*4+5]<-as.numeric(loss)
  }}


for (k in 1:28){
  for (i in 1:239){
    qual<-subset(get(paste0(cancerNames[k],2)),start_position<=endSeq[i]&start_position>=startSeq[i])
    gain<-mean(as.numeric(qual$Gain))
    loss<-mean(as.numeric(qual$Loss))
    dataFrame2[i,k*4+6]<-as.numeric(gain-mean(dataFrame2[,1*4+5],na.rm=TRUE))/sd(dataFrame2[,1*4+5],na.rm=TRUE)
    dataFrame2[i,k*4+7]<-as.numeric(loss-mean(dataFrame2[,1*4+5],na.rm=TRUE))/sd(dataFrame2[,1*4+5],na.rm=TRUE)
  } 
}

#Does the same thing as above, except for all cancer types across each chromosome. 

for (i in 1:239){
  quan<-subset(get(paste0("cancerChromosome",2,"Gain")),start_position<=endSeq[i]&start_position>=startSeq[i])
  gain<-mean(as.numeric(quan$n))
  dataFrame2[i,4]<-as.numeric(gain)
  quav<-subset(get(paste0("cancerChromosome",2,"Loss")),start_position<=endSeq[i]&start_position>=startSeq[i])
  loss<-mean(as.numeric(quav$n))
  dataFrame2[i,5]<-as.numeric(loss)
  
}

for (i in 1:239){
  quan<-subset(get(paste0("cancerChromosome",2,"Gain")),start_position<=endSeq[i]&start_position>=startSeq[i])
  gain<-mean(as.numeric(quan$n))
  loss<-mean(as.numeric(quav$n))
  dataFrame2[i,6]<-((gain-mean(dataFrame2[,4],na.rm=TRUE))/sd(dataFrame2[,4],na.rm=TRUE))
  dataFrame2[i,7]<-((loss-mean(dataFrame2[,5],na.rm=TRUE))/sd(dataFrame2[,5],na.rm=TRUE))
}


