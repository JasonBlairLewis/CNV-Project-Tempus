#initializes data frame 12 with columns and rows based upon length of chromosome 
dataFrame12<-as.data.frame(matrix(ncol=119,nrow=130))
colnames(dataFrame12)<-c("Chromosome","Start Position","End Position","All Cancers Gain","All Cancers Loss","All Cancer Gain Z Score","All Cancer Loss Z Score","pancreaticCancerGain","pancreaticCancerLoss","pancreaticCancerGainZScore","pancreaticCancerLossZScore","stomachCancerGain","stomachCancerLoss","stomachCancerGainZScore","stomachCancerLossZScore","centralNervousSystemGain","centralNervousSystemCancerLoss","centralNervousSystemGainZScore","centralNervousSystemLossZScore","lungCancerGain","lungCancerLoss","lungCancerGainZScore","lungCancerLossZScore","breastCancerGain","breastCancerLoss","breastCancerGainZScore","breastCancerLossZScore","haematopoieticandlymphoidtissueCancerGain","haematopoieticandlymphoidtissueCancerLoss", "haematopoieticandlymphoidtissueCancerGainZscore","haematopoieticandlymphoidtissueCancerLossZscore","largeIntestineCancerGain","largeIntestineCancerLoss","largeIntestineCancerGainZScore","largeIntestineCancerLossZScore","skinCancerGain","skinCancerLoss","skinCancerGainZScore","skinCancerLossZScore","ovarianCancerGain","ovarianCancerLoss","ovarianCancerGainZScore","ovarianCancerLossZScore","kidneyCancerGain","kidneyCancerLoss","kidneyCancerGainZScore","kidneyCancerLossZScore","upperAerodigestiveTractCancerGain","upperAerodigestiveCancerLoss", "upperAerodigestiveCancerGainZScore", "upperAerodigestiveCancerLossZScore", "oesophagusCancerGain","oesophagusCancerLoss","oesophagusCancerGainZScore","oesophagusCancerLossZScore","liverCancerGain","liverCancerLoss","liverCancerGainZScore","liverCancerLossZScore","endometrialCancerGain","endometrialCancerLoss","endometrialCancerGainZScore","endometrialCancerLossZScore","prostateCancerGain","prostateCancerLoss","prostateCancerGainZScore","prostateCancerLossZScore","thyroidCancerGain","thyroidCancerLoss","thyroidCancerGainZScore","thyroidCancerLossZScore","softTissueCancerGain","softTissueCancerLoss","softTissueCancerGainZScore","softTissueCancerLossZScore","testicularCancerGain","testicularCancerLoss","testicularCancerGainZScore","testicularCancerLossZScore","pleuralCancerGain","pleuralCancerLoss","pleuralCancerGainZScore","pleuralCancerLossZScore","boneCancerGain","boneCancerLoss","boneCancerGainZScore","boneCancerLossZScore","autonomicGangliaCancerGain","autonomicGangliaCancerLoss","autonomicGanliaCancerGainZScore","autonomicGangliaCancerLossZScore","thymusCancerGain","thymusCancerLoss","thymusCancerGainZScore","thymusCancerLossZScore","eyeCancerGain","eyeCancerLoss","eyeCancerGainZScore","eyeCancerLossZScore","NSCancerGain","NSCancerLoss","NSCancerGainZScore","NSCancerLossZScore","vulvaCancerGain","vulvaCancerLoss","vulvaCancerGainZScore","vulvaCancerLossZScore","salivaryGlandCancerGain","salivaryGlandCancerLoss","salivaryGlandCancerGainZScore","salivaryGlandCancerLossZSvore","placentalCancerGain","placentalCancerLoss","placentalCancerGainZScore","placentalCancerLossZScore","smallIntestineCancerGain","smallIntestineCancerLoss","smallIntestineCancerGainZScore","smallIntestineCancerLossZScore")                            
dataFrame12$"Chromosome"<-12
startSeq<-seq(0,134000000,1000000)
endSeq<-seq(5000000,134000000,1000000)
for (p in 1:130){
  scipen<-999
  dataFrame12$`Start Position`[p]<-startSeq[p]
  dataFrame12$`End Position`[p]<-endSeq[p]
}


#qual subsets data from a specific cancer type and specific chromosome by position on chromosome to create a smaller data frame of 
#of dat from only genes with start positions between a 5,000,000 bp  window, and then finds the mean of the Gain/Loss column of that subset and Assigns a column in data frame 
#respective data. 

for (k in 1:28){
  for (i in 1:130){
    qual<-subset(get(paste0(cancerNames[k],12)),start_position<=endSeq[i]&start_position>=startSeq[i])
    print(qual)
    gain<-mean(as.numeric(qual$Gain))
    loss<-mean(as.numeric(qual$Loss))
    dataFrame12[i,k*4+4]<-as.numeric(gain)
    dataFrame12[i,k*4+5]<-as.numeric(loss)
  }}


for (k in 1:28){
  for (i in 1:130){
    qual<-subset(get(paste0(cancerNames[k],12)),start_position<=endSeq[i]&start_position>=startSeq[i])
    gain<-mean(as.numeric(qual$Gain))
    loss<-mean(as.numeric(qual$Loss))
    dataFrame12[i,k*4+6]<-as.numeric(gain-mean(dataFrame12[,1*4+5],na.rm=TRUE))/sd(dataFrame12[,1*4+5],na.rm=TRUE)
    dataFrame12[i,k*4+7]<-as.numeric(loss-mean(dataFrame12[,1*4+5],na.rm=TRUE))/sd(dataFrame12[,1*4+5],na.rm=TRUE)
  } 
}

#Does the same thing as above, except for all cancer types across each chromosome. 

for (i in 1:130){
  quan<-subset(get(paste0("cancerChromosome",12,"Gain")),start_position<=endSeq[i]&start_position>=startSeq[i])
  gain<-mean(as.numeric(quan$n))
  dataFrame12[i,4]<-as.numeric(gain)
  quav<-subset(get(paste0("cancerChromosome",12,"Loss")),start_position<=endSeq[i]&start_position>=startSeq[i])
  loss<-mean(as.numeric(quav$n))
  dataFrame12[i,5]<-as.numeric(loss)
  
}

for (i in 1:130){
  quan<-subset(get(paste0("cancerChromosome",12,"Gain")),start_position<=endSeq[i]&start_position>=startSeq[i])
  gain<-mean(as.numeric(quan$n))
  loss<-mean(as.numeric(quav$n))
  dataFrame12[i,6]<-((gain-mean(dataFrame12[,4],na.rm=TRUE))/sd(dataFrame12[,4],na.rm=TRUE))
  dataFrame12[i,7]<-((loss-mean(dataFrame12[,5],na.rm=TRUE))/sd(dataFrame12[,5],na.rm=TRUE))
}



