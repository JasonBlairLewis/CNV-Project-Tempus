  quan<-subset(get(paste0("cancerChromosome",3,"Gain")),start_position<=endSeq[i]&start_position>=startSeq[i])
  gain<-mean(as.numeric(quan$n))
  dataFrame3[i,4]<-as.numeric(gain)
  quav<-subset(get(paste0("cancerChromosome",3,"Loss")),start_position<=endSeq[i]&start_position>=startSeq[i])
  loss<-mean(as.numeric(quav$n))
  dataFrame3[i,5]<-as.numeric(loss)
  
