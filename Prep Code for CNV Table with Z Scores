#qual subsets data from a specific cancer type and specific chromosome by position on chromosome to create a smaller data frame of 
#of dat from only genes with start positions between a 5,000,000 bp  window, and then finds the mean of the Gain/Loss column of that subset and Assigns a column in data frame 
#respective data. 

for (k in 1:28){
  for (i in 1:187){
    qual<-subset(get(paste0(cancerNames[k],4)),start_position<=endSeq[i]&start_position>=startSeq[i])
    print(qual)
    gain<-mean(as.numeric(qual$Gain))
    loss<-mean(as.numeric(qual$Loss))
    dataFrame4[i,k*4+4]<-as.numeric(gain)
    dataFrame4[i,k*4+5]<-as.numeric(loss)
  }}


for (k in 1:28){
  for (i in 1:187){
    qual<-subset(get(paste0(cancerNames[k],4)),start_position<=endSeq[i]&start_position>=startSeq[i])
    gain<-mean(as.numeric(qual$Gain))
    loss<-mean(as.numeric(qual$Loss))
    dataFrame4[i,k*4+6]<-as.numeric(gain-mean(dataFrame4[,1*4+5],na.rm=TRUE))/sd(dataFrame4[,1*4+5],na.rm=TRUE)
    dataFrame4[i,k*4+7]<-as.numeric(loss-mean(dataFrame4[,1*4+5],na.rm=TRUE))/sd(dataFrame4[,1*4+5],na.rm=TRUE)
  } 
}

