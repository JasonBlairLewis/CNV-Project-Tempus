  dataFrame22[i,7]<-((loss-mean(dataFrame22[,5],na.rm=TRUE))/sd(dataFrame22[,5],na.rm=TRUE))
}




#binds all these tables together into one large data frame 
completeCNVTables<-abs(rbind(dataFrame,dataFrame2,dataFrame3,dataFrame4,dataFrame5,dataFrame6,dataFrame7,dataFrame8,dataFrame9,dataFrame10,dataFrame11,dataFrame12,dataFrame13,dataFrame14,dataFrame15,dataFrame16,dataFrame17,dataFrame18,dataFrame19,dataFrame20,dataFrame21,dataFrame22))
write.csv(completeCNVTables,file="Complete CNV Table")
