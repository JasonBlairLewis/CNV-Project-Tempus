  loss<-mean(as.numeric(quav$n))
  dataFrame7[i,6]<-((gain-mean(dataFrame7[,4],na.rm=TRUE))/sd(dataFrame7[,4],na.rm=TRUE))
  dataFrame7[i,7]<-((loss-mean(dataFrame7[,5],na.rm=TRUE))/sd(dataFrame7[,5],na.rm=TRUE))
}


