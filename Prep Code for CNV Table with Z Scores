placental_Cancer<-filter(byCancerSubset,primarySite=="placenta")
stomach_Cancer<-filter(byCancerSubset,primarySite=="stomach")
bone_Cancer<-filter(byCancerSubset,primarySite=="bone")

#initializes cancer names to be used later 
cancerNames<-c("pancreasCancer","stomachCancer","centralNervousSystemCancer","lungCancer","breastCancer","haematopoieticandlymphoidtissueCancer","largeIntestineCancer","skinCancer","ovarianCancer","kidneyCancer","upperAerodigestiveTractCancer","oesophagusCancer","liverCancer","endometrialCancer","prostateCancer","thyroidCancer","softTissueCancer","testicularCancer","pleuralCancer","boneCancer","autonomicGangliaCancer","thymusCancer","eyeCancer","NSCancer","vulvaCancer","salivaryGlandCancer","placentalCancer","smallIntestineCancer")
#name<-list(pancreasCancer,stomachCancer,centralNervousSystemCancer,lungCancer,breastCancer,haematopoieticandlymphoidtissueCancer,largeIntestineCancer, skinCancer, ovarianCancer, kidneyCancer, upperAerodigestiveTractCancer, oesophagusCancer, liverCancer, endometrialCancer, prostateCancer,thyroidCancer,softTissueCancer,testicularCancer,pleuralCancer,boneCancer,autonomicGangliaCancer,thymusCancer,eyeCancer,NSCancer,biliaryTractCancer,vulvaCancer,salivaryGlandCancer,placentalCancer,smallIntestineCancer)
numbersGain<-list(pancreas_Cancer,stomach_Cancer,central_Nervous_System_Cancer,lung_Cancer,breast_Cancer,haematopoietic_and_lymphoid_tissue_Cancer,large_Intestine_Cancer,skin_Cancer,ovarian_Cancer,kidney_Cancer,upper_Aerodigestive_Tract_Cancer,oesophagus_Cancer,liver_Cancer,endometrial_Cancer,prostate_Cancer,thyroid_Cancer,soft_Tissue_Cancer,testicular_Cancer,pleural_Cancer,bone_Cancer,autonomic_Ganglia_Cancer,thymus_Cancer,eye_Cancer,NS_Cancer,vulva_Cancer,salivary_Gland_Cancer,placental_Cancer,small_Intestine_Cancer,urinary_Tract_Cancer,liver_Cancer,endometrial_Cancer,thyroid_Cancer,cervical_Cancer,adrenal_Gland_Cancer)
numbersLoss<-list(pancreas_Cancer,stomach_Cancer,central_Nervous_System_Cancer,lung_Cancer,breast_Cancer,haematopoietic_and_lymphoid_tissue_Cancer,large_Intestine_Cancer,skin_Cancer,ovarian_Cancer,kidney_Cancer,upper_Aerodigestive_Tract_Cancer,oesophagus_Cancer,liver_Cancer,endometrial_Cancer,prostate_Cancer,thyroid_Cancer,soft_Tissue_Cancer,testicular_Cancer,pleural_Cancer,bone_Cancer,autonomic_Ganglia_Cancer,thymus_Cancer,eye_Cancer,NS_Cancer,vulva_Cancer,salivary_Gland_Cancer,placental_Cancer,small_Intestine_Cancer,urinary_Tract_Cancer,liver_Cancer,endometrial_Cancer,thyroid_Cancer,cervical_Cancer,adrenal_Gland_Cancer)
#adds column names from larger subset
geneNames<-byCancerSubset$gene_name





for (i in 1:28){
  j<-assign(paste0("Gain",cancerNames[i]), filter(numbersGain[[i]],MUT_TYPE=="gain"))
  p<-assign(paste0("Loss",cancerNames[i]), filter(numbersLoss[[i]],MUT_TYPE=="loss"))
  q<-full_join(j,p,by="gene_name")
  q[is.na(q)]<-0
  colnames(q)<-c("gene_name","NA","NA","Gain","NA","NA","Loss")
  assign(cancerNames[i],q[c("gene_name","Gain","Loss")])
}

##########################################################################################################
#initializes data frame 1 with columns and rows based upon length of chromosome 
#(for all of the next similar chunks, the rows represent the amount of base pairs times 1 million are contained in that chromosome 
#in order to cycle through each base pair interval until the last million interval)

dataFrame<-as.data.frame(matrix(ncol=119,nrow=246))
colnames(dataFrame)<-c("Chromosome","Start Position","End Position","All Cancers Gain","All Cancers Loss","All Cancer Gain Z Score","All Cancer Loss Z Score","pancreaticCancerGain","pancreaticCancerLoss","pancreaticCancerGainZScore","pancreaticCancerLossZScore","stomachCancerGain","stomachCancerLoss","stomachCancerGainZScore","stomachCancerLossZScore","centralNervousSystemGain","centralNervousSystemCancerLoss","centralNervousSystemGainZScore","centralNervousSystemLossZScore","lungCancerGain","lungCancerLoss","lungCancerGainZScore","lungCancerLossZScore","breastCancerGain","breastCancerLoss","breastCancerGainZScore","breastCancerLossZScore","haematopoieticandlymphoidtissueCancerGain","haematopoieticandlymphoidtissueCancerLoss", "haematopoieticandlymphoidtissueCancerGainZscore","haematopoieticandlymphoidtissueCancerLossZscore","largeIntestineCancerGain","largeIntestineCancerLoss","largeIntestineCancerGainZScore","largeIntestineCancerLossZScore","skinCancerGain","skinCancerLoss","skinCancerGainZScore","skinCancerLossZScore","ovarianCancerGain","ovarianCancerLoss","ovarianCancerGainZScore","ovarianCancerLossZScore","kidneyCancerGain","kidneyCancerLoss","kidneyCancerGainZScore","kidneyCancerLossZScore","upperAerodigestiveTractCancerGain","upperAerodigestiveCancerLoss", "upperAerodigestiveCancerGainZScore", "upperAerodigestiveCancerLossZScore", "oesophagusCancerGain","oesophagusCancerLoss","oesophagusCancerGainZScore","oesophagusCancerLossZScore","liverCancerGain","liverCancerLoss","liverCancerGainZScore","liverCancerLossZScore","endometrialCancerGain","endometrialCancerLoss","endometrialCancerGainZScore","endometrialCancerLossZScore","prostateCancerGain","prostateCancerLoss","prostateCancerGainZScore","prostateCancerLossZScore","thyroidCancerGain","thyroidCancerLoss","thyroidCancerGainZScore","thyroidCancerLossZScore","softTissueCancerGain","softTissueCancerLoss","softTissueCancerGainZScore","softTissueCancerLossZScore","testicularCancerGain","testicularCancerLoss","testicularCancerGainZScore","testicularCancerLossZScore","pleuralCancerGain","pleuralCancerLoss","pleuralCancerGainZScore","pleuralCancerLossZScore","boneCancerGain","boneCancerLoss","boneCancerGainZScore","boneCancerLossZScore","autonomicGangliaCancerGain","autonomicGangliaCancerLoss","autonomicGanliaCancerGainZScore","autonomicGangliaCancerLossZScore","thymusCancerGain","thymusCancerLoss","thymusCancerGainZScore","thymusCancerLossZScore","eyeCancerGain","eyeCancerLoss","eyeCancerGainZScore","eyeCancerLossZScore","NSCancerGain","NSCancerLoss","NSCancerGainZScore","NSCancerLossZScore","vulvaCancerGain","vulvaCancerLoss","vulvaCancerGainZScore","vulvaCancerLossZScore","salivaryGlandCancerGain","salivaryGlandCancerLoss","salivaryGlandCancerGainZScore","salivaryGlandCancerLossZSvore","placentalCancerGain","placentalCancerLoss","placentalCancerGainZScore","placentalCancerLossZScore","smallIntestineCancerGain","smallIntestineCancerLoss","smallIntestineCancerGainZScore","smallIntestineCancerLossZScore")                            
dataFrame$"Chromosome"<-1
startSeq<-seq(0,250000000,1000000)
endSeq<-seq(5000000,250000000,1000000)
for (p in 1:246){
  scipen<-999
  dataFrame$`Start Position`[p]<-startSeq[p]
  dataFrame$`End Position`[p]<-endSeq[p]
}


#qual subsets data from a specific cancer type and specific chromosome by position on chromosome to create a smaller data frame of 
#of data from only genes with start positions between a 5,000,000 bp  window, and then finds the mean of the Gain/Loss column of that subset and Assigns a column in data frame 
#respective data. print function inside for loop is unnecessary.

#1:28 is the amount of cancer types
#the second for loop (for all of the next similar chunks represent the amount of base pairs times 1 million are contained in that chromosome )
for (k in 1:28){
  for (i in 1:246){
    qual<-subset(get(paste0(cancerNames[k],1)),start_position<=endSeq[i]&start_position>=startSeq[i])
    print(qual)
    gain<-mean(as.numeric(qual$Gain))
    loss<-mean(as.numeric(qual$Loss))
    dataFrame[i,k*4+4]<-as.numeric(gain)
    dataFrame[i,k*4+5]<-as.numeric(loss)
}}


for (k in 1:28){
  for (i in 1:246){
    qual<-subset(get(paste0(cancerNames[k],1)),start_position<=endSeq[i]&start_position>=startSeq[i])
    gain<-mean(as.numeric(qual$Gain))
    loss<-mean(as.numeric(qual$Loss))
dataFrame[i,k*4+6]<-as.numeric(gain-mean(dataFrame[,1*4+5],na.rm=TRUE))/sd(dataFrame[,1*4+5],na.rm=TRUE)
dataFrame[i,k*4+7]<-as.numeric(loss-mean(dataFrame[,1*4+5],na.rm=TRUE))/sd(dataFrame[,1*4+5],na.rm=TRUE)
  } 
    }

#Does the same thing as above, except for all cancer types across each chromosome. 

for (i in 1:246){
  quan<-subset(get(paste0("cancerChromosome",1,"Gain")),start_position<=endSeq[i]&start_position>=startSeq[i])
  gain<-mean(as.numeric(quan$n))
  dataFrame[i,4]<-as.numeric(gain)
  quav<-subset(get(paste0("cancerChromosome",1,"Loss")),start_position<=endSeq[i]&start_position>=startSeq[i])
  loss<-mean(as.numeric(quav$n))
  dataFrame[i,5]<-as.numeric(loss)

}

for (i in 1:246){
  quan<-subset(get(paste0("cancerChromosome",1,"Gain")),start_position<=endSeq[i]&start_position>=startSeq[i])
  gain<-mean(as.numeric(quan$n))
  loss<-mean(as.numeric(quav$n))
dataFrame[i,6]<-((gain-mean(dataFrame[,4],na.rm=TRUE))/sd(dataFrame[,4],na.rm=TRUE))
dataFrame[i,7]<-((loss-mean(dataFrame[,5],na.rm=TRUE))/sd(dataFrame[,5],na.rm=TRUE))
}


