###################################
######        SHINY      APP       1
library(shiny)
library(plotly)

ui<-basicPage(
  titlePanel("Basic widgets"),
  sidebarLayout(
    sidebarPanel(
      selectizeInput("cancerType","Cancer",choices=c("pancreasCancer","stomachCancer","centralNervousSystemCancer","lungCancer","breastCancer","haematopoieticandlymphoidtissueCancer","largeIntestineCancer","skinCancer","ovarianCancer","kidneyCancer","upperAerodigestiveTractCancer","oesophagusCancer","liverCancer","endometrialCancer","prostateCancer","thyroidCancer","softTissueCancer","testicularCancer","pleuralCancer","boneCancer","autonomicGangliaCancer","thymusCancer","eyeCancer","NSCancer","biliaryTractCancer","vulvaCancer","salivaryGlandCancer","placentalCancer","smallIntestineCancer")),
                                            
      selectInput("gene","Gene Name",choices="ADAM18"),
      
      numericInput("num", "Upper Threshold" 
                   ,100),
      numericInput("num2", "Lower Threshold"
                   ,100),
      submitButton("Submit")),
    
    mainPanel(
      plotOutput('plot'))
  ))

server <- function(output,input) {


  cancerType<-reactive({
    x<-get(input$cancerType)
  })
  gene<- reactive({
  
    
  })
  output$plot <- renderPlot({
    # cancerType<- subset(cancerType(),Gain >= input$num)
    # cancerType<- subset(cancerType(),Loss >= input$num2)
    # cancerType<-subset(cancerType(),gene_name="input$gene")
    # cancerType<-cancerType()[grep((input$gene),cancerType()),]
    z<-ggplot(cancerType(), aes(x=start_position, y=Gain),size=5) + 
      ylim(-200,200) +
      geom_point()+
      geom_point(aes(y = -Loss)) +
      xlab("Gene Name")+ylab("Copy Number Variable")+ggtitle(paste(as.character(input$cancerType)))+
      theme(plot.title = element_text(hjust = .5 ,size=20))
print(z)
  })
    }

shinyApp(ui,server)

pancreasCancer<-(left_join(pancreasCancer,chromosomes,by=c(gene_name="hgnc_symbol")))

d<-c("pancreasCancer1","pancreasCancer2","pancreasCancer3","pancreasCancer4","pancreasCancer5","pancreasCancer6","pancreasCancer7","pancreasCancer8","pancreasCancer9","pancreasCancer10","pancreasCancer11","pancreasCancer12","pancreasCancer13","pancreasCancer14","pancreasCancer15","pancreasCancer16","pancreasCancer17","pancreasCancer18","pancreasCancer19","pancreasCancer20", "pancreasCancer21","pancreasCancer22")
for (i in 1:22){
  assign(d[i],data.frame(pancreasCancer[c(grep(paste("\\b",i,"\\b",sep=""),pancreasCancer$chromosome_name)),]))
  assign(d[i],arrange(((get(d[i]))),start_position))
  assign(d[i],(get(d[i])[c("gene_name","Gain","Loss","start_position")]))
}
  
  pancreasCancerPlots<-c("pancreasCancerPlot1","pancreasCancerPlot2","pancreasCancerPlot3","pancreasCancerPlot4","pancreasCancerPlot5","pancreasCancerPlot6","pancreasCancerPlot7","pancreasCancerPlot8","pancreasCancerPlot9","pancreasCancerPlot10","pancreasCancerPlot11","pancreasCancerPlot12","pancreasCancerPlot13","pancreasCancerPlot14","pancreasCancerPlot15","pancreasCancerPlot16","pancreasCancerPlot17","pancreasCancerPlot18","pancreasCancerPlot19","pancreasCancerPlot20","pancreasCancerPlot21","pancreasCancerPlot22")
for (i in 1:22){
  assign(pancreasCancerPlots[i],
         (ggplot(get(d[i]),aes(start_position,Gain),size=.5)+
            geom_point()+
            geom_point(aes(y=-Loss),col="red")+
            xlab("Gene Start Position")+
            ylab("Copy Number Variable")+ggtitle(paste0("chromosome",i))+
            theme(plot.title = element_text(hjust = .5 ,size=14.5,face="bold"))+
            theme(axis.text=element_text(size=10),
                  axis.title=element_text(size=10,face="bold"))))
}
  
##############################################
         dsex<-c("pancreasCancerX","pancreasCancerY")
         
         for (i in LETTERS[24:25]){
           assign(dsex[i],data.frame(pancreasCancer[c(grep(paste("\\b",i,"\\b",sep=""),pancreasCancer$chromosome_name)),]))
           assign(dsex[i],arrange(((get(dsex[i]))),start_position))
           assign(dsex[i],(get(dsex[i])[c("gene_name","Gain","Loss","start_position")]))
         }
pancreasCancerSexPlots<-c("pancreasCancerPlotX","pancreasCancerPlotY")


for (i in LETTERS[24:25]){
         assign(paste0("pancreasCancerPlot",i),
                (ggplot(get(dsex[i]),aes(start_position,Gain),size=.5)+
                   geom_point()+
                   geom_point(aes(y=-Loss),color="red")+
                   xlab("Gene Start Position")+
                   ylab("Copy Number Variable")+ggtitle(paste0("chromosome",i))+
                   theme(plot.title = element_text(hjust = .5 ,size=7.5,face="bold"))+
                   theme(axis.text=element_text(size=5),
                         axis.title=element_text(size=5,face="bold"))))
  }

pancreasCancerChromosomesPlot<-gridExtra::grid.arrange(pancreasCancerPlot1,pancreasCancerPlot2,pancreasCancerPlot3,pancreasCancerPlot4,pancreasCancerPlot5,pancreasCancerPlot6,pancreasCancerPlot7,pancreasCancerPlot8,pancreasCancerPlot9,pancreasCancerPlot10,pancreasCancerPlot11,pancreasCancerPlot12,pancreasCancerPlot13,pancreasCancerPlot14,pancreasCancerPlot15,pancreasCancerPlot16,pancreasCancerPlot17,pancreasCancerPlot18,pancreasCancerPlot19,pancreasCancerPlot20,pancreasCancerPlot21,pancreasCancerPlot22,pancreasCancerPlotX,pancreasCancerPlotY,nrow=6,ncol=4)

