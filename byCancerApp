for (x in 1:10) {local({
  i <- x
  output[[paste('result', i, sep='')]] <- reactive(function(){
    if (input[[paste('run', i, sep='')]] == 0)
      return()
    return(isolate({
      runCode(input[[paste('code', i, sep='')]])
    }))
  })
})}

