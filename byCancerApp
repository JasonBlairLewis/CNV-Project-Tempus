library(shiny)
USC<-fluidPage(
  titlePanel("Pancreatic Cancer Chromosome Data"),
  mainPanel(
    selectizeInput("cancerType","Cancer",choices=c("pancreasCancer","stomachCancer","centralNervousSystemCancer","lungCancer","breastCancer","haematopoieticandlymphoidtissueCancer","largeIntestineCancer","skinCancer","ovarianCancer","kidneyCancer","upperAerodigestiveTractCancer","oesophagusCancer","liverCancer","endometrialCancer","prostateCancer","thyroidCancer","softTissueCancer","testicularCancer","pleuralCancer","boneCancer","autonomicGangliaCancer","thymusCancer","eyeCancer","NSCancer","biliaryTractCancer","vulvaCancer","salivaryGlandCancer","placentalCancer","smallIntestineCancer")),
    
    conditionalPanel(
      condition = "input.cancerType =='pancreasCancer'",
      selectInput("geneName", "Gene Name",
                  choices=(pancreasCancer$gene_name)
      )),
    
          plotOutput("graph1", width="150%"),
          plotOutput("graph2",width="150%"),
          plotOutput("graph3",width="150%"),
          plotOutput("graph4",width="150%"),
          plotOutput("graph5",width="150%"),
          plotOutput("graph6",width="150%"),
          plotOutput("graph7",width="150%"),
          plotOutput("graph8", width="150%"),
          plotOutput("graph9",width="150%"),
          plotOutput("graph10",width="150%"),
          plotOutput("graph11",width="150%"),
          plotOutput("graph12",width="150%"),
          plotOutput("graph13",width="150%"),
          plotOutput("graph14",width="150%"),
          plotOutput("graph15",width="150%"),
          plotOutput("graph16",width="150%"),
          plotOutput("graph17",width="150%"),
          plotOutput("graph18",width="150%"),
          plotOutput("graph19",width="150%"),
          plotOutput("graph20",width="150%"),
          plotOutput("graph21",width="150%"),
          plotOutput("graph22",width="150%")
          # plotOutput("graphX",width="150%"),
          # plotOutput("graphY",width="150%")


    ))
apple<-function(output,input){
  
  
  cancerType<-reactive({
    cancerType<-input$cancerType
  })
  
  
  choices<-list("pancreasCancerPlot","stomachCancerPlot","centralNervousSystemCancerPlot","lungCancerPlot","breastCancerPlot","haematopoieticandlymphoidtissueCancerPlot","largeIntestineCancerPlot","skinCancerPlot","ovarianCancerPlot","kidneyCancerPlot","upperAerodigestiveTractCancerPlot","oesophagusCancerPlot","liverCancerPlot","endometrialCancerPlot","prostateCancerPlot","thyroidCancerPlot","softTissueCancerPlot","testicularCancerPlot","pleuralCancerPlot","boneCancerPlot","autonomicGangliaCancerPlot","thymusCancerPlot","eyeCancerPlot","NSCancerPlot","biliaryTractCancerPlot","vulvaCancerPlot","salivaryGlandCancerPlot","placentalCancerPlot","smallIntestineCancerPlot")


  ##############################################
  #dsex<-c("pancreasCancerX","pancreasCancerY")
  
  # for (i in LETTERS[24:25]){
  #   assign(dsex[i],data.frame(pancreasCancer[c(grep(paste("\\b",i,"\\b",sep=""),pancreasCancer$chromosome_name)),]))
  #   assign(dsex[i],arrange(((get(dsex[i]))),start_position))
  #   assign(dsex[i],(get(dsex[i])[c("gene_name","Gain","Loss","start_position")]))
  # }
  # 
  # pancreasCancerSexPlots<-c("pancreasCancerPlotX","pancreasCancerPlotY")
  # 
  # 
  # for (i in LETTERS[24:25]){
  #   assign(paste0("pancreasCancerPlot",i),
  #          (ggplot(get(dsex[i]),aes(start_position,Gain),size=.5)+
  #             geom_point()+
  #             geom_point(aes(y=-Loss),color="red")+
  #             xlab("Gene Start Position")+
  #             ylab("Copy Number Variable")+ggtitle(paste0("chromosome",i))+
